{
  "name": "react-router-middleware",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "dev:react-router": "react-router dev",
    "dev:db": "docker compose -f docker/docker-compose.yml up -d && npm run prisma:migrate",
    "build": "react-router build",
    "deploy": "npm run build && wrangler deploy",
    "start": "react-router build && wrangler dev",
    "typecheck": "react-router typegen && tsc -b",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset"
  },
  "dependencies": {
    "@prisma/adapter-pg": "6.4.1",
    "@prisma/client": "6.4.1",
    "@react-router/node": "^7.3.0",
    "@react-router/serve": "^7.3.0",
    "isbot": "^5.1.23",
    "pg": "8.13.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.3.0",
    "remix-provider": "1.0.3"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "0.1.9",
    "@cloudflare/workers-types": "^4.20250310.0",
    "@react-router/dev": "^7.3.0",
    "@tailwindcss/vite": "^4.0.12",
    "@types/node": "^22.13.10",
    "@types/pg": "8.11.11",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "npm-run-all": "4.1.5",
    "prisma": "6.4.1",
    "tailwindcss": "^4.0.12",
    "typescript": "^5.8.2",
    "vite": "^6.2.1",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^3.114.0"
  },
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/SoraKumo001/react-router-middleware",
  "author": "SoraKumo <info@croud.jp>",
  "license": "MIT"
}
